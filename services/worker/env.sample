# Copy to .env for local development
APP_ENV=local

# LocalStack gateway (recommended)
AWS_ENDPOINT_URL=http://127.0.0.1:4566
AWS_REGION=ap-southeast-2
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# Domain queue URL (adjust workspace prefix if needed)
CREWVIA_DOMAIN_EVENT_QUEUE=http://127.0.0.1:4566/000000000000/default-crewvia-domain-event-queue

# Optional: Postgres persistence
DATABASE_URL=postgresql+psycopg://dbmaster:dbpassword@127.0.0.1:5432/application
